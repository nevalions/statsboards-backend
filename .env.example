LOGS_CONFIG="logging-config_dev.yaml"
CURRENT_SEASON_ID=

DB_HOST=
DB_PORT=5432
DB_USER=
DB_PASSWORD=
DB_NAME=

DB_TEST_HOST=localhost
DB_TEST_USER=test
DB_TEST_PASSWORD=test
DB_TEST_NAME=test_db
DB_TEST_PORT=5432

# Rate limiting settings
# Maximum requests per second (default: 2.0)
RATE_LIMIT_REQUESTS_PER_SECOND=2.0

# Maximum concurrent requests (default: 5)
RATE_LIMIT_MAX_CONCURRENT=5

# Proxy source URLs (optional)
# Comma-separated list of URLs to fetch proxy lists from
# If PROXY_LIST is not provided, proxies will be fetched from these URLs
# Examples:
#   - GitHub: https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt
#   - ProxyScrape: https://api.proxyscrape.com/?request=displayproxies&proxytype=http&country=RU
#   - Multiple: "https://example.com/proxies.txt,https://another.com/proxies.txt"
# Default (fetches from GitHub): https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt
PROXY_SOURCE_URLS=

# Proxy source cache TTL in seconds (default: 3600 = 1 hour)
PROXY_SOURCE_CACHE_TTL=3600

# Maximum number of proxies to fetch from sources (default: 100)
# Prevents overwhelming the system with thousands of proxies
PROXY_SOURCE_MAX_PROXIES=100

# Proxy source fetch timeout in seconds (default: 10)
PROXY_SOURCE_FETCH_TIMEOUT=10

# Proxy connection timeout in seconds (default: 10)
PROXY_TIMEOUT=10

# Maximum retry attempts when a proxy fails (default: 3)
PROXY_MAX_RETRIES=3

# Proxy source URLs (optional)
# Comma-separated list of URLs to fetch proxy lists from
# Examples:
#   - GitHub: https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt
#   - ProxyScrape: https://api.proxyscrape.com/?request=displayproxies&proxytype=http&country=RU
#   - Multiple: "https://example.com/proxies.txt,https://another.com/proxies.txt"
PROXY_SOURCE_URLS=

# Proxy source cache TTL in seconds (default: 3600 = 1 hour)
PROXY_SOURCE_CACHE_TTL=3600

# Maximum number of proxies to fetch from sources (default: 100)
# Prevents overwhelming the system with thousands of proxies
PROXY_SOURCE_MAX_PROXIES=100

# Proxy source fetch timeout in seconds (default: 10)
PROXY_SOURCE_FETCH_TIMEOUT=10
