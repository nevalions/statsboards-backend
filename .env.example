LOGS_CONFIG="logging-config_dev.yaml"
CURRENT_SEASON_ID=

DB_HOST=
DB_PORT=
DB_USER=
DB_PASSWORD=
DB_NAME=

DB_TEST_HOST=
DB_TEST_USER=
DB_TEST_PASSWORD=
DB_TEST_NAME=
DB_TEST_PORT=

# Rate limiting settings
# Maximum requests per second (default: 2.0)
RATE_LIMIT_REQUESTS_PER_SECOND=2.0

# Maximum concurrent requests (default: 5)
RATE_LIMIT_MAX_CONCURRENT=5

# Proxy settings (optional)
# Comma-separated list of proxy URLs
# Supports formats:
#   - socks5://user:pass@host:port
#   - socks5://host:port
#   - http://user:pass@host:port
#   - http://host:port
# Example: PROXY_LIST="socks5://user:pass@proxy1.example.com:8080,socks5://user:pass@proxy2.example.com:8081"

# Proxy connection timeout in seconds (default: 10)
PROXY_TIMEOUT=10

# Maximum retry attempts when a proxy fails (default: 3)
PROXY_MAX_RETRIES=3

# Proxy source URLs (optional)
# Comma-separated list of URLs to fetch proxy lists from
# Examples:
#   - SOCKS5: https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/socks5.txt
#   - SOCKS5: https://raw.githubusercontent.com/hookzof/socks5_list/master/proxy.txt
#   - HTTP: https://raw.githubusercontent.com/TheSpeedX/PROXY-List/master/http.txt
#   - Multiple: "https://example.com/socks5.txt,https://another.com/socks5.txt"

# Proxy source cache TTL in seconds (default: 3600 = 1 hour)
PROXY_SOURCE_CACHE_TTL=3600

# Maximum number of proxies to fetch from sources (default: 100)
# Prevents overwhelming the system with thousands of proxies
PROXY_SOURCE_MAX_PROXIES=100

# Proxy source fetch timeout in seconds (default: 10)
PROXY_SOURCE_FETCH_TIMEOUT=10

# Proxy connection timeout in seconds (default: 10)
PROXY_TIMEOUT=10

# Maximum retry attempts when a proxy fails (default: 3)
PROXY_MAX_RETRIES=3
